             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Mocassin.UI.GUI.MainControls.ProjectBrowser"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance local:ProjectBrowserViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
