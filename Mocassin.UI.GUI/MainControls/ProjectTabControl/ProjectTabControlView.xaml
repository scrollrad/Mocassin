<UserControl x:Class="Mocassin.UI.GUI.MainControls.ProjectTabControl.ProjectTabControlView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Mocassin.UI.GUI.MainControls.ProjectTabControl"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance local:ProjectTabControlViewModel, IsDesignTimeCreatable=True}"
             Height="Auto" Width="Auto">

  <Grid Margin="0,0,0,0">
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="*" />
    </Grid.ColumnDefinitions>
    <Grid.RowDefinitions>
      <RowDefinition Height="*" />
    </Grid.RowDefinitions>
  </Grid>
</UserControl>
