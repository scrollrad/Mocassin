<UserControl
  x:Class="Mocassin.UI.GUI.Controls.ProjectWorkControl.SubControls.TransitionModel.TransitionModelControlView"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:local="clr-namespace:Mocassin.UI.GUI.Controls.ProjectWorkControl.SubControls.TransitionModel"
  xmlns:dataControl="clr-namespace:Mocassin.UI.GUI.Controls.ProjectWorkControl.SubControls.TransitionModel.DataControl"
  d:DataContext="{d:DesignInstance local:TransitionModelControlViewModel, IsDesignTimeCreatable=True}"
  mc:Ignorable="d"
  d:DesignHeight="450" d:DesignWidth="800"
  Background="Transparent">
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="*" />
      <RowDefinition Height="*" />
    </Grid.RowDefinitions>

    <dataControl:AbstractTransitionControlView Grid.Row="0" DataContext="{Binding AbstractTransitionControlViewModel}" />
    <dataControl:TransitionControlView Grid.Row="1" DataContext="{Binding TransitionControlViewModel}" />
  </Grid>
</UserControl>
